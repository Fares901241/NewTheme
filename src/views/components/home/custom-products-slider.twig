<div class="container my-5">
  <div class="row">
    <div class="col-12 col-md-6">
      <h2 class="mb-3"{{component.title}}</h2>
    </div>
  </div>
  <div class="row">
    <div class="col-12 slider-container-card">
      <div id="cardCarousel" class="custom-carousel">
        <div class="custom-carousel-images">
          {% for product in component.products %}
            <a href="{{product.url}}">
              <div class="card image-item text-left text-start card-count position-relative" style="height:auto">
                <img src="{{product.image.url}}" class="card-img-top second-slider-img" alt="Image 1">
                <div class="m-2" style="justify-self: end;height: fit-content; position: absolute; right:0">
                  <div class="bg-light rounded-5 p-2">
                    <div class="heart-container" style="width: 15px; height: 15px;" title="Like">
                      <input type="checkbox" class="checkbox" id="Give-It-An-Id">
                      <div class="svg-container">
                        <svg viewBox="0 0 24 24" class="svg-outline" xmlns="http://www.w3.org/2000/svg">
                          <path
                            d="M17.5,1.917a6.4,6.4,0,0,0-5.5,3.3,6.4,6.4,0,0,0-5.5-3.3A6.8,6.8,0,0,0,0,8.967c0,4.547,4.786,9.513,8.8,12.88a4.974,4.974,0,0,0,6.4,0C19.214,18.48,24,13.514,24,8.967A6.8,6.8,0,0,0,17.5,1.917Zm-3.585,18.4a2.973,2.973,0,0,1-3.83,0C4.947,16.006,2,11.87,2,8.967a4.8,4.8,0,0,1,4.5-5.05A4.8,4.8,0,0,1,11,8.967a1,1,0,0,0,2,0,4.8,4.8,0,0,1,4.5-5.05A4.8,4.8,0,0,1,22,8.967C22,11.87,19.053,16.006,13.915,20.313Z">
                          </path>
                        </svg>
                        <svg viewBox="0 0 24 24" class="svg-filled" xmlns="http://www.w3.org/2000/svg">
                          <path
                            d="M17.5,1.917a6.4,6.4,0,0,0-5.5,3.3,6.4,6.4,0,0,0-5.5-3.3A6.8,6.8,0,0,0,0,8.967c0,4.547,4.786,9.513,8.8,12.88a4.974,4.974,0,0,0,6.4,0C19.214,18.48,24,13.514,24,8.967A6.8,6.8,0,0,0,17.5,1.917Z">
                          </path>
                        </svg>
                        <svg class="svg-celebrate" width="100" height="100" xmlns="http://www.w3.org/2000/svg">
                          <polygon points="10,10 20,20"></polygon>
                          <polygon points="10,50 20,50"></polygon>
                          <polygon points="20,80 30,70"></polygon>
                          <polygon points="90,10 80,20"></polygon>
                          <polygon points="90,50 80,50"></polygon>
                          <polygon points="80,80 70,70"></polygon>
                        </svg>
                      </div>
                    </div>
                  </div>
                </div>
                <div class="card-body">
                  <p class="card-text text-end">{{product.name}}</p>
                  <p class="price text-primary text-end">السعر: {{product.price}} ر.س</p>
                </div>
              </div>
            </a>
          {% endfor %}
        </div>
        <button class="custom-prev-button-card btn btn-primary p-0">
          <i class="bi bi-chevron-left text-white"></i>
        </button>
        <button class="custom-next-button-card btn btn-primary p-0">
          <i class="bi bi-chevron-right text-white"></i>
        </button>
      </div>
    </div>
  </div>
</div>